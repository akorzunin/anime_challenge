{% extends 'base.jinja2' %}
{%block title%} Ebalo  {%endblock%}
{% block styles %} 
table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
}

td, th {
    border: 1px solid #c5bebe;
    text-align: left;
    padding: 8px;
}

tr:nth-child(even) {
    background-color: #dbdbdb;
}
{%endblock%}
{%block content%} 
<div class="container">
    {{ content|safe }}
</div>
{%endblock%}